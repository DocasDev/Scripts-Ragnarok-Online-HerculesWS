gef_tower,44,104,4	script	Roger#ica	4_M_YOYOROGUE,{
	.@npc_name$ = "^FF8B00[= Roger =]^000000";
	if(BaseLevel < 90){
		mes .@npc_name$;
		mes "Ora ora, você não me parece ser o tipo de pessoa que deveria me procurar.";
		mes "Se você fosse mais forte, quem sabe...";
		close;
	}
	if (questprogress(61807)) {
		if(questprogress(61807, PLAYTIME) == 1){
			mes .@npc_name$;
			mes "Vá com calma. Não é tão simples abrir a Fenda Dimensional. Teremos que aguardar um tempo.";
			close;
		}
		erasequest(61807);
	}
	mes .@npc_name$;
	mes "Eaí!";
	mes "Você gosta de apostar?";
	next;
	if(select("Não:Sim") == 1){
		mes .@npc_name$;
		mes "Então isso aqui não é para você.";
		close;
	}
	mes .@npc_name$;
	mes "Eu também amo apostar.";
	mes "Você apostaria a sua vida?";
	next;
	if(select("Não:Se valer à pena") == 1){
		mes .@npc_name$;
		mes "Então isso aqui não é para você.";
		close;
	}
	mes .@npc_name$;
	mes "Ora ora, quem diria que eu encontraria alguém como você nessa Dimensão.";
	mes "Estou começando a gostar de você! Hehe";
	next;
	mes .@npc_name$;
	mes "Enfim!";
	mes "Eu conheço um lugar em que você pode conseguir muitas recompensas, se sovreviver.";
	mes "Estaria interessado?";
	next;
	if(select("Não:Claro") == 1){
		mes .@npc_name$;
		mes "É...";
		mes "Você não parece preparado para isso.";
		close;
	}
	mes .@npc_name$;
	mes "Encontrei esse lugar durante minhas viagens por outras dimensões.";
	mes "É uma espécie de arena com um cristal que dizem ser amaldiçoado.";
	next;
	mes .@npc_name$;
	mes "Ele consegue invocar criaturas de várias dimensões.";
	mes "Ouvi dizer que foi criado por um mago bizarro que gostava de apostas.";
	next;
	mes .@npc_name$;
	mes "Existem várias rodadas e em cada uma delas o cristal irá invocar diversas criaturas.";
	mes "A cada rodada que você sobrevive, você ganha uma quantidade de ^0075E9" + getitemname(Crystal_Of_Feardoom_GT) + "^000000.";
	next;
	mes .@npc_name$;
	mes "O mago de quem te falei, está na sua Dimensão.";
	mes "Ele recebe esse cristal em troca de alguns itens.";
	next;
	mes .@npc_name$;
	mes "Eaí?";
	mes "Está afim de tentar? Eu posso te mostrar onde fica.";
	next;
	if(select("Depois:Claro") == 1){
		mes .@npc_name$;
		mes "Tudo bem...";
		mes "Você quem sabe.";
		close;
	}
	.@party_id = getcharid(CHAR_ID_PARTY);
	if (!.@party_id) {
		mes .@npc_name$;
		mes "Você não está em um Grupo. Dessa forma meu dispositivo não irá funcionar.";
		close;
	}
	if (getcharid(CHAR_ID_CHAR) != getpartyleader(.@party_id, 2)) {
		mes .@npc_name$;
		mes "A sua empolgação é bacana, mas terei que pedir para falar com o Líder do seu Grupo.";
		close;
	}
	.@instance = instance_create("Cristal Amaldiçoado", .@party_id);
	if (.@instance >= 0) {
		if (instance_attachmap("1@sthd", .@instance) == "") {
			mes .@npc_name$;
			mes "Meu dispositivo está com mau funcionamento...";
			mes "Talvez esteja sobrecarregado.";
			mes "Vamos tentar de novo em alguns minutos.";
			close2;
			instance_destroy(.@instance);
			end;
		}
		instance_set_timeout(3600, 10, .@instance);
		instance_init(.@instance);
	}
	if(.@instance < 0 || has_instance("1@sthd") == ""){
		mes .@npc_name$;
		mes "Meu dispositivo está com mau funcionamento...";
		mes "Talvez esteja sobrecarregado.";
		mes "Vamos tentar de novo em alguns minutos.";
		close;
	}
	mes .@npc_name$;
	mes "Muito bem, tudo está pronto.";
	mes "Boa sorte ^0075E9" + strcharinfo(PC_NAME) + "^000000!";
	close2;
	setquest(61807);
	warp("1@sthd", 98, 71);
	end;
}

