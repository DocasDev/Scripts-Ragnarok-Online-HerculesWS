//======================
//== Loja
//======================
-	shop	loja_medo_ica	-1,501:100

gef_tower,66,175,4	script	Loja do Medo#ica	4_MAN_NITT,{
	.nome_npc$ = "^FF8B00[= Loja do Medo =]^000000";
	mes .nome_npc$;
	mes "Olá, ^0075E9" + strcharinfo(PC_NAME) + "^000000!";
	mes "Os itens que vendo podem ser comprados com ^0075E9" + getitemname(Crystal_Of_Feardoom_GT) + "^000000, obtidos na instância ^0075E9Cristal Amaldiçoado^000000.";
	mes "^FF0000A Skill de desconto não funciona nesse shop^000000";
	close2;
	npcshopattach(.shopname$, 1);
	callshop .shopname$, 1;
	end;

OnBuyItem:
	@total = 0;
	.@k = 0;
	for(.@i = 0; .@i < getarraysize(@bought_nameid); .@i++){
		for(.@j = 0; .@j < getarraysize(.itens); .@j += 2){
			if(@bought_nameid[.@i] == .itens[.@j]){
				setarray(.@buying[.@k], .itens[.@j], @bought_quantity[.@i]);
				.@total += .itens[.@j + 1] * @bought_quantity[.@i];
				.@k += 2;
				break;
			}
		}
	}
	if(countitem(Crystal_Of_Feardoom_GT) < .@total){
		dispbottom("Você não tem " + getitemname(Crystal_Of_Feardoom_GT) + " suficiente.", 0xFF0000);
		end;
	}
	delitem(Crystal_Of_Feardoom_GT, .@total);
	dispbottom("Você gastou " + .@total + "x " + getitemname(Crystal_Of_Feardoom_GT) + ".");
	for(.@i = 0; .@i < getarraysize(.@buying); .@i += 2){
		getitem(.@buying[.@i], .@buying[.@i + 1]);
	}
	end;

OnInit:
	deletearray(.itens[0], getarraysize(.itens));
	setarray(.itens[0],
		Thanos_Sword,			2000,
		Thanos_Great_Sword,		2000,
		Thanos_Dagger,			2000,
		Thanos_Katar,			2000,
		Thanos_Hammer,			2000,
		Thanos_Axe,				2000,
		Thanos_Knuckle,			2000,
		Thanos_Long_Spear,		2000,
		Thanos_Spear,			2000,
		Thanos_Staff,			2000,
		Thanos_Rod,				2000,
		Thanos_Violin,			2000,
		Thanos_Whip,			2000,
		
		Armor_Of_Gray,			1000,
		Boots_Of_Gray,			1000,
		Cloak_Of_Gray,			1000,
		Gray_Helmet,			1000,
		Gray_Robe,				1000,
		Shield_Of_Gray,			1000
	);
	
	.shopname$ = "loja_medo_ica";
	npcshopdelitem .shopname$, 501;
	
	for(.@i = 0; .@i < getarraysize(.itens); .@i += 2){
		npcshopadditem .shopname$, .itens[.@i], .itens[.@i + 1];
	}
	end;
}

